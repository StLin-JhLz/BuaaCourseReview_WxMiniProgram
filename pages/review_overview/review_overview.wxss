.homeBox {
    width: 80%;
    margin: 5% auto;
  }

  .block {
    margin: 0 32rpx;
  }
  
  .block + .block {
    margin-top: 48rpx;
  }
  
  .block .margin-16:not(:last-child) {
    margin-right: 32rpx;
  }

.review{
    margin: 3% auto;
}

.button-example {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    margin: 0 32rpx;
  }
  

  .wrapper {
    flex-wrap: wrap;
    margin: 0 32rpx;
  }
  
  .wrapper t-check-tag {
    display: block;
  }
  
  .block {
    display: flex;
    font-size: 14px;
    align-items: center;
    line-height: 76rpx;
    color: rgba(0, 0, 0, 0.4);
  }
  
  .block text {
    display: block;
    width: 160rpx;
    margin-right: 32rpx;
  }
  
